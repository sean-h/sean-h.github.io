<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Sean Humeniuk" %></title>
    
    <%= stylesheet_link_tag "normalize", "all", "bootstrap", "octicons/octicons", "jquery.fancybox.css?v=2.1.5" %>

  </head>
  
  <body class="<%= page_classes %>">
    <%= partial "header" %> 
    <%= yield %>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <%= javascript_include_tag  "all", "bootstrap", "jquery-1.10.1.min", "jquery.fancybox.pack" %>
    <script type="text/javascript">
      $(document).ready(function() {
            $(".fancybox").fancybox();
              });
    </script>
  </body>
</html>
