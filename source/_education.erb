<div class="container section">
  <h2>Education</h2>
  <div class="item">
    <div class="education">
      <% data.education.listings.each do |ed| %>
        <div class="education-image">
          <img src="/images/<%= ed.image %>"/>
        </div>
        <div class="education-info">
          <div class="education-school"><%= ed.school %></div>
          <div class="education-degree"><%= ed.degree %></div>
        </div>
      <% end %>
    </div>
  </div>
</div>
